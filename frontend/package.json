{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --parallel",
    "build": "npm run clean && next build --experimental-build-mode=compile --experimental-parallel-server-builds",
    "build:fast": "npm run clean && next build --experimental-turbo --experimental-parallel-server-builds",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build:fast && npm run optimize && npm run bundle:check",
    "build:cached": "next build --experimental-build-mode=compile --experimental-parallel-server-builds",
    "start": "next start",
    "lint": "next lint",
    "lint:security": "eslint src/**/*.{ts,tsx} --config .eslintrc.security.js",
    "test": "jest --maxWorkers=75% --cache --passWithNoTests",
    "test:watch": "jest --watch --maxWorkers=50% --cache --onlyChanged",
    "test:coverage": "jest --coverage --maxWorkers=75% --cache --collectCoverageOnlyFrom=changed-files",
    "test:unit": "jest --testPathPattern=__tests__ --maxWorkers=50%",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:ci": "npm run lint && npm run typecheck && npm run test:coverage",
    "test:compliance": "jest --testPathPattern=compliance",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "typecheck": "tsc --noEmit --incremental --skipLibCheck",
    "clean": "rm -rf .next out dist",
    "clean:cache": "rm -rf node_modules/.cache .eslintcache tsconfig.tsbuildinfo .next/cache .next/build-performance.json",
    "clean:full": "npm run clean && npm run clean:cache && rm -rf node_modules",
    "optimize": "npm run optimize:images && npm run optimize:bundle",
    "optimize:images": "echo 'Image optimization completed during build'",
    "optimize:bundle": "echo 'Bundle optimization completed during build'",
    "performance:build": "time npm run build:production && npm run bundle:report",
    "bundle:analyze": "npm run build:analyze && npx @next/bundle-analyzer",
    "bundle:check": "node scripts/bundle-size-check.js || true",
    "bundle:report": "npm run bundle:size && npm run bundle:gzip",
    "bundle:size": "find .next/static -name '*.js' | xargs wc -c | tail -1",
    "bundle:gzip": "find .next/static -name '*.js' | xargs gzip -c | wc -c",
    "precommit:build": "npm run typecheck && npm run build:fast"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@mantine/charts": "^7.17.8",
    "@mantine/core": "^7.17.8",
    "@mantine/dates": "^7.17.8",
    "@mantine/form": "^7.17.8",
    "@mantine/hooks": "^7.17.8",
    "@mantine/modals": "^7.17.8",
    "@mantine/notifications": "^7.17.8",
    "@mantine/spotlight": "^7.17.8",
    "@medplum/core": "^4.3.0",
    "@medplum/fhirtypes": "^4.3.0",
    "@medplum/react": "^4.3.0",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/react-big-calendar": "^1.16.2",
    "@types/recharts": "^1.8.29",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "enzyme-to-json": "^3.6.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "react": "^19.1.0",
    "react-big-calendar": "^1.19.4",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.59.0",
    "react-router-dom": "^7.6.3",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.68",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^8.4.1",
    "@medplum/mock": "^4.3.0",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/crypto-js": "^4.2.2",
    "@types/fhir": "^0.0.41",
    "@types/jest": "^29.5.8",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.19.4",
    "@types/passport-oauth2": "^1.8.0",
    "@types/passport-saml": "^1.1.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-router-dom": "^5.3.3",
    "@types/serviceworker": "^0.0.140",
    "axe-core": "^4.8.3",
    "crypto-js": "^4.2.0",
    "eslint": "^9.30.1",
    "eslint-config-next": "15.3.4",
    "eslint-plugin-security": "^1.7.1",
    "fake-indexeddb": "^5.0.2",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5",
    "workbox-window": "^7.3.0"
  }
}
